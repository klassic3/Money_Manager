{
  "expo": {
    "name": "Money_Manager",
    "slug": "Money_Manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MM.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MM.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.klassic3.Money_Manager"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/MM.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/MM.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "61436f59-ab81-47a1-86c8-b05175a3d15a"
      }
    },
    "owner": "klassic3"
  }
}
